{
  "name": "sinnerschrader-career",
  "version": "0.1.0",
  "description": "sinnerschrader career page generated with handlebars and markdown",
  "scripts": {
    "build": "ts-node src/index.ts build",
    "start": "ts-node src/index.ts serve",
    "serve": "nodemon"
  },
  "author": "martin.sonnenholzer",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^14.14.2",
    "chalk": "^4.1.0",
    "globby": "^11.0.1",
    "gray-matter": "^4.0.2",
    "handlebars": "^4.7.6",
    "nodemon": "^2.0.6",
    "rehype-stringify": "^8.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "serve-handler": "^6.1.3",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3",
    "unified": "^9.2.0",
    "yaml": "^1.10.0"
  },
  "nodemonConfig": {
    "ignore": [
      "docs/*"
    ],
    "watch": [
      "template",
      "data"
    ],
    "ext": "ts html css md",
    "delay": 500
  }
}
